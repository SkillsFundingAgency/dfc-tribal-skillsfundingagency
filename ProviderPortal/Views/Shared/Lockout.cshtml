@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = AppGlobal.Language.GetText(this, "Title", "Locked Out");
}

<hgroup>
    <h1 class="text-danger">@AppGlobal.Language.GetText(this, "Header", "Locked out")</h1>
    <h2 class="text-danger">@AppGlobal.Language.GetText(this, "Message", "This account has been locked out, please try again later.")</h2>
</hgroup>
